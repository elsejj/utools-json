{
  "logo": "favicon.png",
  "main": "index.html",
  "features": [
    {
      "code": "json",
      "cmds": [
        "json",
        {
          "type": "regex",
          "minLength": 20,
          "match": "/^\\s*(\\{[\\s\\S]*\\}|\\[[\\s\\S]*\\])\\s*$/",
          "label": "Json"
        },
        {
          "type": "regex",
          "minLength": 20,
          "match": "/^(['\"`][\\s\\S+]+['\"`])$/",
          "label": "QuotedJson"
        },
        {
          "type": "regex",
          "minLength": 20,
          "match": "/^\\s*</",
          "label": "XML"
        },
        {
          "type": "regex",
          "minLength": 20,
          "match": "/^(?:[A-Za-z0-9+/][A-Za-z0-9+/][A-Za-z0-9+/][A-Za-z0-9+/])+(?:[A-Za-z0-9+/]{2}==|[A-Za-z0-9+/]{3}=)?$/",
          "label": "Base64 JSON"
        },
        {
          "type": "over",
          "minLength": 20,
          "label": "YAML/Toml/..."
        }
      ],
      "explain": "全能的JSON处理工具"
    }
  ],
  "name": "JsonPlus",
  "pluginName": "全能JSON处理",
  "description": "一个全能的JSON处理工具，添加了官方JSON编辑器所缺失的功能",
  "author": "Good",
  "homepage": "https://github.com/elsejj/utools-json",
  "version": "1.0.19"
}
